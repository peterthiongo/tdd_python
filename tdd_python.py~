#!/usr/bin/env python

import unittest

class calculator(object):

    def add (self, x, y):
        return x+y

class TddInPythonExample(unittest.TestCase):

    def test_calculator_add_method_returns_correct_result(self):
        calc = Calculator()
        result = calc.add(2, 2)
        self.assertEqual(4, result)
        print result

